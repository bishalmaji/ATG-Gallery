<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="32dp"
        android:right="32dp"
        android:top="32dp"
        android:bottom="32dp">
        <shape android:shape="oval">
            <stroke android:color="?attr/colorPrimary" android:width="4dp" android:dashWidth="8dp" android:dashGap="8dp"/>

            <size
                android:width="120dp"
                android:height="120dp"/>
        </shape>
    </item></layer-list>
